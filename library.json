{
    "name": "Tempo",
    "version": "1.5.0",
    "description": "Bibliothèque Arduino pour créer des temporisations non bloquantes avec gestion des unités de temps (micro, millisecondes, secondes, minutes, heures). Possibilité de Pause, Restart et ajout de CallBack",
    "keywords": [
      "timer",
      "delay",
      "antidelay",
      "minuterie",
      "temporisation",
      "tempo",
      "non-blocking",
      "millis",
      "micros",
      "arduino"
    ],
    "repository":
    {
      "type": "git",
      "url": "https://github.com/boby15000/Tempo.git"
    },
    "authors":
    [
      {
        "name": "Nicolas Fourgheon",
        "email": "boby15000@hotmail.com",
        "url": "https://github.com/boby15000/Tempo",
        "maintainer": true
      }
    ],
    "examples": [
      {
        "name": "oneTimer",
        "base": "exemples/",
        "files": ["oneTimer.cpp"]
      },
      {
        "name": "moreTimer",
        "base": "exemples/",
        "files": ["moreTimer.cpp"]
      },
      {
        "name": "PauseTimer",
        "base": "exemples/",
        "files": ["PauseTimer.cpp"]
      },
      {
        "name": "CallBackTimer",
        "base": "exemples/",
        "files": ["CallBackTimer.cpp"]
      }
    ],
    "license": "MIT",
    "homepage": "https://github.com/boby15000/Tempo",
    "frameworks": "arduino",
    "platforms": [
      "atmelavr",
      "espressif32",
      "espressif8266",
      "ststm32"
    ],
    "export": {
      "exclude": [
        "lib/",
        "src/main.cpp",
        "test/",
        ".gitignore",
        "platformio.ini"
      ]
    }
